    <div class="news-detail-con simditor">
        <div class="news-detail-wrap clearfix">
            <div class="text-header">
                <h6 style="text-align: <%= newsData.current.title.length > 28 ? 'left' : 'center' %>;">
                    <%= !newsData.current || !newsData.current.title ? '' : newsData.current.title %>
                </h6>
                <div class="issue-box"><p class="author"><span><%= newsData.current.author %></span></p>
                    <p class="time">发布时间：<span>
                            <%
                            const format = (date, str) => {
                                let _str = !str ? '-' : str
                                const zero = (m) => {
                                    return m < 10 ? '0' + m : m
                                }
                                let time = new Date(date)
                                let y = time.getFullYear()
                                let m = time.getMonth() + 1
                                let d = time.getDate()
                                if (date) {
                                    return y + _str + zero(m) + _str + zero(d)
                                } else {
                                    return ''
                                }
                            }
                            %>
                            <%= format(newsData.current. createTime)%>
                        </span></p>
                    <p class="keyword">关键字：<span><%= newsData.current.tags %></span></p>
                    <p data-ifCollect="<%= newsData.ifCollect %>" class="collect">
                    <% if(newsData.ifCollect === 1){ %>
                    <i class="iconfont hasCollect">&#xe605;</i>
                    <span> 已收藏</span>
                    <% } else { %>
                        <i class="iconfont">&#xe651;</i>
                        <span> 收藏</span>
                    <% } %>
                    </p>
                    <p class="read-numbner"><span><%= newsData.current.hotCounts %></span></p>
                </div>
            </div>
            <div class="details-synopsis active" id="detailsSynopsis">
                <p><%= newsData.current.synopsis %></p>
            </div>
            <!-- 音频播放 -->
